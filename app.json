{
  "expo": {
    "name": "Narria",
    "slug": "narria-audiobook-player",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/narria-logo-big-n.png",
    "scheme": "narriaaudiobookplayer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/narria-logo-big-n.png",
        "resizeMode": "contain",
        "backgroundColor": "#002b64"
      },
      "bundleIdentifier": "com.nyllaah.narriaaudiobookplayer",
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#002b64",
        "foregroundImage": "./assets/images/narria-logo-big-n.png",
        "resizeMode": "contain"
      },
      "splash": {
        "image": "./assets/images/narria-logo-big-n.png",
        "resizeMode": "contain",
        "backgroundColor": "#002b64"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.nyllaah.narriaaudiobookplayer",
      "permissions": [
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_MEDIA_PLAYBACK"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/narria-logo-big-n.png",
          "resizeMode": "contain",
          "backgroundColor": "#002b64",
          "dark": {
            "backgroundColor": "#002b64"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9614d215-674c-486e-afcb-d4db86c2cb81"
      }
    }
  }
}
